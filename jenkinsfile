pipeline {
    agent any
tools {
        maven 'Maven 3.0.5'
        jdk 'jdk8'
    }
    stages {
        stage('1, KKK Example') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('2, Maven build'){
        steps {
                sh mvn install -DskipTests
            }
        }
        stage('3, Maven compiling'){
        steps {
                sh mvn compile
            }
        }
    }
}
