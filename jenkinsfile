pipeline {
    agent any

    stages {
        stage('1, test proj') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('2, Maven build'){
        steps {
                sh 'mvn install -DskipTests'
            }
        }
        stage('3, Maven compiling'){
        steps {
                sh 'mvn compile'
            }
        }
    }
}
